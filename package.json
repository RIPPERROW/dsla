{
  "name": "stacktical-dsla-contracts",
  "version": "2.0.0",
  "description": "DSLA Protocol",
  "main": "index.js",
  "scripts": {
    "test:local": "NODE_ENV=test TEST_ENV=local truffle test --network local",
    "test:local:specific": "NODE_ENV=test TEST_ENV=local truffle test --network local test/bdsla.test.js",
    "test:chainlink": "NODE_ENV=test TEST_ENV=chainlink truffle test --network chainlink",
    "test:kovan": "NODE_ENV=test TEST_ENV=kovan truffle test --network kovan",
    "deploy:kovan": "truffle deploy --network kovan && npm run export-data",
    "deploy:kovan:reset": "truffle deploy --reset --network kovan && npm run export-data",
    "deploy:local": "truffle deploy --network local && npm run export-data",
    "deploy:local:reset": "truffle deploy --reset --network local && npm run export-data",
    "remix": "npx remixd -s . --remix-ide https://remix.ethereum.org",
    "compile": "truffle compile",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "export-data": "npm run export-data:scripts && prettier --config .prettierrc.abis.json --write 'exported_data/**.ts'",
    "export-data:scripts": "npm run compile && npm run export-data:abis && npm run export-data:addresses",
    "export-data:abis": "truffle exec --network local scripts/export-abis.js",
    "export-data:addresses": "truffle exec --network local scripts/export-addresses.js"
  },
  "author": "DSLA Core Developers",
  "license": "ISC",
  "dependencies": {
    "@chainlink/contracts": "0.0.11",
    "@openzeppelin/contracts": "^3.2.0",
    "prettier": "^2.2.1",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.9",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stacktical/stacktical-dsla-contracts.git"
  },
  "bugs": {
    "url": "https://github.com/Stacktical/stacktical-dsla-contracts/issues"
  },
  "homepage": "https://github.com/Stacktical/stacktical-dsla-contracts#readme"
}
