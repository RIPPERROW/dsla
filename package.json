{
  "name": "stacktical-dsla-contracts",
  "version": "2.0.0",
  "description": "DSLA Protocol",
  "main": "index.js",
  "scripts": {
    "deploy:kovan:reset": "NODE_ENV=kovan npm run truffle:deploy:kovan:reset && npm run export-data",
    "deploy:develop": "NODE_ENV=develop npm run truffle:deploy:develop && npm run export-data",
    "deploy:develop:reset": "NODE_ENV=develop npm run truffle:deploy:develop:reset && npm run export-data",
    "deploy:develop:tokens": "NODE_ENV=develop npm run truffle:deploy:develop:tokens && npm run truffle:exec:export-addresses && npm run prettier:write:exported-data",
    "deploy:develop:tokens:reset": "NODE_ENV=develop npm run truffle:deploy:develop:tokens:reset && npm run truffle:exec:export-addresses && npm run prettier:write:exported-data",
    "deploy:staging:tokens": "NODE_ENV=staging npm run truffle:deploy:staging:tokens && npm run truffle:exec:export-addresses:staging && npm run prettier:write:exported-data",
    "deploy:staging:tokens:reset": "NODE_ENV=staging npm run truffle:deploy:staging:tokens:reset && npm run truffle:exec:export-addresses:staging && npm run prettier:write:exported-data",
    "export-data": "npm run export-data:scripts && npm run prettier:write:exported-data && npm run prettier:write:natspec-docs",
    "mint-tokens": "npm run truffle:exec:mint-bdsla && npm run truffle:exec:mint-dai",
    "test:kovan": "NODE_ENV=kovan TEST_ENV=true npm run truffle:test:kovan",
    "test:develop": "NODE_ENV=develop TEST_ENV=true npm run truffle:test:develop",
    "test:develop:bdsla": "NODE_ENV=develop TEST_ENV=true npm run truffle:test:develop test/bdsla.test.js",
    "test:develop:messenger": "NODE_ENV=develop TEST_ENV=true npm run test:develop test/messenger.test.js",
    "test:develop:slaRegistry": "NODE_ENV=develop TEST_ENV=true npm run truffle:test:develop test/slaRegistry.test.js",
    "test:develop:sla": "NODE_ENV=develop TEST_ENV=true npm run truffle:test:develop test/sla.test.js",
    "test:staging": "NODE_ENV=staging TEST_ENV=true npm run truffle:test:staging",
    "test:staging:bdsla": "NODE_ENV=staging TEST_ENV=true npm run truffle:test:staging test/bdsla.test.js",
    "test:staging:messenger": "NODE_ENV=staging TEST_ENV=true npm run truffle:test:staging test/messenger.test.js",
    "test:staging:sla": "NODE_ENV=staging TEST_ENV=true npm run truffle:test:staging test/sla.test.js",
    "test:staging:slaRegistry": "NODE_ENV=staging TEST_ENV=true npm run truffle:test:staging test/slaRegistry.test.js",
    "export-data:scripts": "npm run truffle:compile && npm run truffle:exec:export-abis && npm run truffle:exec:export-addresses && npm run truffle:exec:create-natspec-docs",
    "fund-messenger:develop": "NODE_ENV=develop npm run truffle:exec:fund-messenger",
    "request-sli:develop": "NODE_ENV=develop npm run truffle:exec:requestSLI",
    "eslint:fix:js": "eslint --fix \"**/*.js\" --ignore-pattern node_modules/",
    "prettier:contracts": "prettier --write 'contracts/**/*.sol'",
    "prettier:write:natspec-docs": "prettier --write 'natspec-docs/**/**.json'",
    "prettier:write:exported-data": "prettier --config .prettierrc.abis.json --write 'exported-data/**.ts'",
    "truffle:compile": "truffle compile",
    "truffle:deploy:develop": "truffle deploy --network develop",
    "truffle:deploy:develop:reset": "truffle deploy --reset --network develop",
    "truffle:deploy:develop:tokens": "DEPLOY_TOKENS=true npm run truffle:deploy:develop",
    "truffle:deploy:develop:tokens:reset": "DEPLOY_TOKENS=true npm run truffle:deploy:develop:reset",
    "truffle:deploy:kovan": "truffle deploy --network kovan",
    "truffle:deploy:kovan:reset": "truffle deploy --reset --network kovan",
    "truffle:deploy:kovan:tokens": "DEPLOY_TOKENS=true npm run truffle:deploy:kovan",
    "truffle:deploy:kovan:tokens:reset": "DEPLOY_TOKENS=true npm run truffle:deploy:kovan:reset",
    "truffle:deploy:staging": "truffle deploy --network staging",
    "truffle:deploy:staging:tokens": "DEPLOY_TOKENS=true npm run truffle:deploy:staging",
    "truffle:deploy:staging:tokens:reset": "DEPLOY_TOKENS=true npm run truffle:deploy:staging:reset",
    "truffle:exec:create-natspec-docs": "truffle exec --network develop scripts/create-natspec-docs.js",
    "truffle:exec:export-abis": "truffle exec --network develop scripts/export-abis.js",
    "truffle:exec:export-addresses": "truffle exec --network develop scripts/export-addresses.js",
    "truffle:exec:export-addresses:staging": "truffle exec --network staging scripts/export-addresses.js",
    "truffle:exec:get-active-pool": "truffle exec --network develop scripts/get-active-pool.js",
    "truffle:exec:get-active-pool:kovan": "truffle exec --network kovan scripts/get-active-pool.js",
    "truffle:exec:mint-bdsla": "truffle exec --network develop scripts/mint-bdsla.js",
    "truffle:exec:stake-bdsla": "truffle exec --network develop scripts/stake-bdsla.js",
    "truffle:exec:mint-bdsla:notOwner": "NOT_OWNER=true truffle exec --network develop scripts/mint-bdsla.js",
    "truffle:exec:stake-bdsla:notOwner": "NOT_OWNER=true truffle exec --network develop scripts/stake-bdsla.js",
    "truffle:exec:mint-bdsla:kovan": "truffle exec --network kovan scripts/mint-bdsla.js",
    "truffle:exec:stake-bdsla:kovan": "truffle exec --network kovan scripts/stake-bdsla.js",
    "truffle:exec:mint-dai": "truffle exec --network develop scripts/mint-dai.js",
    "truffle:exec:stake-dai": "truffle exec --network develop scripts/stake-dai.js",
    "truffle:exec:mint-dai:notOwner": "NOT_OWNER=true truffle exec --network develop scripts/mint-dai.js",
    "truffle:exec:stake-dai:notOwner": "NOT_OWNER=true truffle exec --network develop scripts/stake-dai.js",
    "truffle:exec:stake-dai:kovan": "truffle exec --network kovan scripts/stake-dai.js",
    "truffle:exec:fund-messenger": "truffle exec --network develop scripts/fund-messenger.js",
    "truffle:exec:requestSLI": "truffle exec --network develop scripts/request-sli.js",
    "truffle:console:develop": "truffle console --network develop",
    "truffle:console:kovan": "truffle console --network kovan",
    "truffle:console:staging": "truffle console --network staging",
    "truffle:debug:develop": "truffle debug '0xe399918f36b828c20cc6fed384ffeb1716fac45515df082bfea0ee82628cd366' --network develop",
    "truffle:test:develop": "truffle test --network develop",
    "truffle:test:kovan": "truffle test --network kovan",
    "truffle:test:staging": "truffle test --network staging",
    "remix": "npx remixd -s . --remix-ide https://remix.ethereum.org"
  },
  "author": "DSLA Core Developers",
  "license": "ISC",
  "dependencies": {
    "@chainlink/contracts": "0.0.11",
    "@openzeppelin/contracts": "^3.2.0",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bs58": "^4.0.1",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@types/web3": "^1.2.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-truffle": "^0.3.1",
    "mocha": "^8.2.1",
    "ngrok": "^3.4.0",
    "prettier-plugin-solidity": "^1.0.0-beta.1",
    "web3": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stacktical/stacktical-dsla-contracts.git"
  },
  "bugs": {
    "url": "https://github.com/Stacktical/stacktical-dsla-contracts/issues"
  },
  "homepage": "https://github.com/Stacktical/stacktical-dsla-contracts#readme"
}
