{
  "name": "stacktical-dsla-contracts",
  "version": "2.0.0",
  "description": "DSLA Protocol",
  "main": "index.js",
  "scripts": {
    "test:local": "NODE_ENV=test TEST_ENV=local npm run truffle:test:local",
    "deploy:local": "npm run truffle:deploy:local && npm run export-data",
    "deploy:local:reset": "npm run truffle:deploy:local:reset && npm run export-data",
    "deploy:kovan:reset": "npm run truffle:deploy:kovan:reset && npm run export-data",
    "deploy:local:tokens": "npm run truffle:deploy:local:tokens && npm run truffle:exec:export-addresses && npm run prettier:write:exported-data",
    "deploy:local:tokens:reset": "npm run truffle:deploy:local:tokens:reset && npm run truffle:exec:export-addresses && npm run prettier:write:exported-data",
    "test:kovan": "NODE_ENV=test TEST_ENV=kovan npm run truffle:test:kovan",
    "export-data": "npm run export-data:scripts && npm run prettier:write:exported-data && npm run prettier:write:natspec-docs",
    "mint-tokens": "npm run truffle:exec:mint-bdsla && npm run truffle:exec:mint-dai",
    "test:local:bdsla": "NODE_ENV=test TEST_ENV=local npm run truffle:test:local test/bdsla.test.js",
    "test:local:messenger": "NODE_ENV=test TEST_ENV=local npm run test:local test/messenger.test.js",
    "test:local:slaRegistry": "NODE_ENV=test TEST_ENV=local npm run truffle:test:local test/slaRegistry.test.js",
    "test:local:sla": "NODE_ENV=test TEST_ENV=local npm run truffle:test:local test/sla.test.js",
    "export-data:scripts": "npm run truffle:compile && npm run truffle:exec:export-abis && npm run truffle:exec:export-addresses && npm run truffle:exec:create-natspec-docs",
    "eslint:fix:js": "eslint --fix \"**/*.js\" --ignore-pattern node_modules/",
    "prettier:contracts": "prettier --write 'contracts/**/*.sol'",
    "prettier:write:natspec-docs": "prettier --write 'natspec-docs/**/**.json'",
    "prettier:write:exported-data": "prettier --config .prettierrc.abis.json --write 'exported-data/**.ts'",
    "truffle:compile": "truffle compile",
    "truffle:deploy:local": "truffle deploy --network local",
    "truffle:deploy:local:reset": "truffle deploy --reset --network local",
    "truffle:deploy:local:tokens": "DEPLOY_TOKENS=true npm run truffle:deploy:local",
    "truffle:deploy:local:tokens:reset": "DEPLOY_TOKENS=true npm run truffle:deploy:local:reset",
    "truffle:deploy:kovan": "truffle deploy --network kovan",
    "truffle:deploy:kovan:reset": "truffle deploy --reset --network kovan",
    "truffle:deploy:kovan:tokens": "DEPLOY_TOKENS=true npm run truffle:deploy:kovan",
    "truffle:deploy:kovan:tokens:reset": "DEPLOY_TOKENS=true npm run truffle:deploy:kovan:reset",
    "truffle:exec:create-natspec-docs": "truffle exec --network local scripts/create-natspec-docs.js",
    "truffle:exec:export-abis": "truffle exec --network local scripts/export-abis.js",
    "truffle:exec:export-addresses": "truffle exec --network local scripts/export-addresses.js",
    "truffle:exec:get-active-pool": "truffle exec --network local scripts/get-active-pool.js",
    "truffle:exec:get-active-pool:kovan": "truffle exec --network kovan scripts/get-active-pool.js",
    "truffle:exec:mint-bdsla": "truffle exec --network local scripts/mint-bdsla.js",
    "truffle:exec:mint-bdsla:kovan": "truffle exec --network kovan scripts/mint-bdsla.js",
    "truffle:exec:mint-dai": "truffle exec --network local scripts/mint-dai.js",
    "truffle:exec:stake-dai": "truffle exec --network local scripts/stake-dai.js",
    "truffle:exec:mint-dai:notOwner": "NOT_OWNER=true truffle exec --network local scripts/mint-dai.js",
    "truffle:exec:stake-dai:notOwner": "NOT_OWNER=true truffle exec --network local scripts/stake-dai.js",
    "truffle:console:local": "truffle console --network local",
    "truffle:console:kovan": "truffle console --network kovan",
    "truffle:test:local": "truffle test --network local",
    "truffle:test:kovan": "truffle test --network kovan",
    "remix": "npx remixd -s . --remix-ide https://remix.ethereum.org"
  },
  "author": "DSLA Core Developers",
  "license": "ISC",
  "dependencies": {
    "@chainlink/contracts": "0.0.11",
    "@openzeppelin/contracts": "^3.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "prettier": "^2.2.1",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@types/web3": "^1.2.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-truffle": "^0.3.1",
    "mocha": "^8.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.1",
    "web3": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stacktical/stacktical-dsla-contracts.git"
  },
  "bugs": {
    "url": "https://github.com/Stacktical/stacktical-dsla-contracts/issues"
  },
  "homepage": "https://github.com/Stacktical/stacktical-dsla-contracts#readme"
}
